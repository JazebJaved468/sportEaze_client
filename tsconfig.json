{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
  },
  "include": ["src/**/*", "node_modules/react-native/**/*"], 
  "exclude": ["node_modules"]
}

